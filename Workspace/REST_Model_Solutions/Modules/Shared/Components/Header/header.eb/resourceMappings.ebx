        <fieldMapping resourceField="last_name" field="lastName"/>
        <fieldMapping resourceField="user_id" field="user_id"/>
    </resourceMapping>
    <resourceMapping id="userByUsername">
        <fieldMapping resourceField="username" field="email"/>
        <fieldMapping resourceField="first_name" field="firstName"/>
        <fieldMapping resourceField="last_name" field="lastName"/>
        <fieldMapping resourceField="user_id" field="user_id"/>
    </resourceMapping>
    <resourceMapping id="userEmail">
        <fieldMapping resourceField="type" field="confirmationType"/>
        <fieldMapping resourceField="expiry" field="emailExpiry"/>
        <fieldMapping resourceField="email_uid" field="emailUID"/>
        <fieldMapping resourceField="user_id" field="user_id"/>
    </resourceMapping>
    <resourceMapping id="visibleMenuItems">
        <tableMapping table="visibleMenuItems"/>
        <fieldMapping resourceField="menu_item_id" field="visibleMenuItems-menu_item_id"/>
        <fieldMapping resourceField="userId" field="visibleMenuItems-userId"/>
    </resourceMapping>
</resourceMappings>
